#! /bin/sh

case "$1" in
       remove|upgrade|remove-in-favour|deconfigure-in-favour)
              if [ -f /usr/sbin/dhelp_parse ]; then
                 /usr/sbin/dhelp_parse -d /usr/share/doc/libapt-pkg-doc
              fi
              ;;
       failed-upgrade)
              ;;
esac

#DEBHELPER#
