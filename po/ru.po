#: cmdline/apt-cache.cc:1203 cmdline/apt-cdrom.cc:875 cmdline/apt-config.cc:79
#: cmdline/apt-get.cc:1932
msgid "Commands:"
msgstr "Команды:"

#: cmdline/apt-cache.cc:1204
msgid "   add - Add an package file to the source cache"
msgstr "   add - Добавить пакет в кэш исходных файлов"

#: cmdline/apt-cache.cc:1205
msgid "   gencaches - Build both the package and source cache"
msgstr "   gencaches - Перестроить оба кэша (пакеты и исходники)"

#: cmdline/apt-cache.cc:1206
msgid "   showpkg - Show some general information for a single package"
msgstr "   showpkg - Отобразить общую информацию о пакете"

#: cmdline/apt-cache.cc:1207
msgid "   stats - Show some basic statistics"
msgstr "   stats - Отобразить общую статистику"

#: cmdline/apt-cache.cc:1208
msgid "   dump - Show the entire file in a terse form"
msgstr "   dump - Показать весь файл в сжатом виде"

#: cmdline/apt-cache.cc:1209
msgid "   dumpavail - Print an available file to stdout"
msgstr "   dumpavail - Распечатать доступный файл в stdout"

#: cmdline/apt-cache.cc:1210
msgid "   unmet - Show unmet dependencies"
msgstr "   unmet - Отобразить неудовлетворенные зависимости"

#: cmdline/apt-cache.cc:1211
msgid "   check - Check the cache a bit"
msgstr "   check - Перепроверить кэш"

#: cmdline/apt-cache.cc:1212
msgid "   search - Search the package list for a regex pattern"
msgstr ""
"   search - Искать в списке пакетов с использованием регулярных выражений"

#: cmdline/apt-cache.cc:1213
msgid "   show - Show a readable record for the package"
msgstr "   show - Отобразить информацию о пакете в понятной форме"

#: cmdline/apt-cache.cc:1214
msgid "   depends - Show raw dependency information for a package"
msgstr "   depends - Отобразить всю информацию о зависимостях для пакета"

#: cmdline/apt-cache.cc:1215
msgid "   pkgnames - List the names of all packages"
msgstr "   pkgnames - Отобразить имена всех пакетов"

#: cmdline/apt-cache.cc:1216
msgid "   dotty - Generate package graphs for GraphVis"
msgstr ""
"   dotty - Сгенерировать граф зависимостей пакетов как документ GraphVis"

